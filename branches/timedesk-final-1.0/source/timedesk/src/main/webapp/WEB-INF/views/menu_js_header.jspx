<div id="js_header" xmlns:jsp="http://java.sun.com/JSP/Page" 
                 xmlns:fn="http://java.sun.com/jsp/jstl/functions" 
                 xmlns:c="http://java.sun.com/jsp/jstl/core" 
                 xmlns:spring="http://www.springframework.org/tags" version="2.0">
  <jsp:output omit-xml-declaration="yes" />
  
  <spring:url value="/js/jquery-1.4.2.min.js" var="jquery_js_url"/>
  <spring:url value="/js/menu.js" var="menu_js_url"/>
  
  <script src="${jquery_js_url}" type="text/javascript">;</script>
  <script src="${menu_js_url}" type="text/javascript">;</script>
  <script type="text/javascript">jcode(document).ready(function() {jcode('.menu').initMenu();});</script>
</div>